{
  "name": "zombie-char-component-vue2",
  "version": "1.0.7",
  "author": {
    "name": "Mahdad Ghasemian",
    "email": "mahdad.ghasemian@gmail.com"
  },
  "repository": "https://github.com/MahdadGhasemian/zombie-char-component-vue2",
  "bugs": "https://github.com/MahdadGhasemian/zombie-char-component-vue2/issues",
  "keywords": [
    "zombie",
    "vue2",
    "character",
    "solidity",
    "crypto"
  ],
  "license": "MIT",
  "description": "Vue Zombie Character Component For Vue2",
  "main": "dist/zombie-char-component-vue2.ssr.js",
  "browser": "dist/zombie-char-component-vue2.esm.js",
  "module": "dist/zombie-char-component-vue2.esm.js",
  "unpkg": "dist/zombie-char-component-vue2.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "big-integer": "^1.6.49",
    "js-sha3": "^0.8.0"
  }
}
